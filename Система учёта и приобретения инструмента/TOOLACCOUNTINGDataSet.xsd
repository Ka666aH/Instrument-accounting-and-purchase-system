    n.NomenclatureNumber,
    g.Name,
    n.Designation,
    n.Unit,
    n.Dimensions,
    n.CuttingMaterial,
    n.RegulatoryDoc,
    n.Producer,
    n.UsageFlag,
    n.MinStock,
    -- Добавляем признак типа записи
    CASE 
        WHEN n.NomenclatureNumber = a.OriginalNomenclatureNumber THEN 'Основной'
        ELSE 'Аналог'
    END AS ToolType,
    -- Добавляем поле с номером сортировки (основной инструмент первый)
