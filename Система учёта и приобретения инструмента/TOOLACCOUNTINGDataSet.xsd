    -- Номер инструмента: аналог, если есть, иначе оригинал
    ISNULL(rf.AnalogNomenclatureNumber, rrc.NomenclatureNumber) AS NomenclatureNumber,

    -- Полное наименование инструмента (с учетом аналога)
    COALESCE(g.Name + ' ', '') + 
