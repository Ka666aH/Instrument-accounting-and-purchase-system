INNER JOIN Nomenclature n ON n.NomenclatureNumber = ISNULL(rf.AnalogNomenclatureNumber, rrc.NomenclatureNumber)
LEFT JOIN Groups g ON g.RangeStart = LEFT(n.NomenclatureNumber, 4)

